{
	"browserSyncProxy" : "http://beniaminurbanek.com",
	"browser" : "google chrome",
	"ftpConfig" : "./ftp.config.json",
	"wwwBase": "wp-content/themes/beniaminurbanek/",
	"basePath" : "theme/",
	"paths" : {
		"templates" : {
			"src" : ["./*", "!.*"],
			"dest" : "./"
		},
		"styles" : {
			"src" : "_/styles/*.scss",
			"dest" : "_/styles/dist/"
		},
		"scripts" : {
			"src" : "_/scripts/*.js",
			"dest" : "_/scripts/dist/"
		}
	}
}
